<mvc:View
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<IllustratedMessage illustrationType="sapIllus-PageNotFound"
        title="{i18n>ttlPageNotFound}"
        description="Please check the link or Go to Home Page"
        enableVerticalResponsiveness="true">
        <additionalContent>
            <Button icon="{i18n>iHome}" text="{i18n>btnGoHome}" press="onPressNavigateToPage('Home')"/>
        </additionalContent>
    </IllustratedMessage>
</mvc:View>