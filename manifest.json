{
	"_version": "1.60.0",
	"start_url": "index.html",
	"sap.app": {
		"id": "pharelyshau",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"i18n": {
			"bundleUrl": "i18n/i18n.properties",
			"supportedLocales": ["en", "de", "ru"],
			"fallbackLocale": "en"
		},
		"dataSources": {},
		"offline": true
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"fullWidth": true
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.120.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.f": {},
				"sap.ui.layout": {},
				"sap.ui.unified": {},
				"sap.tnt": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "pharelyshau.i18n.i18n",
					"supportedLocales": ["en", "de", "ru"],
					"fallbackLocale": "en"
				}
			}
		},
		"rootView": {
			"viewName": "pharelyshau.view.App",
			"type": "XML",
			"id": "app",
			"async": true
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"type": "View",
				"viewType": "XML",
				"path": "pharelyshau.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"bypassed": {
					"target": ["NotFound"]
				},
				"async": true
			},
			"routes": [
				{
					"pattern": "",
					"name": "Home",
					"target": "Home"
				},
				{
					"pattern": "resume",
					"name": "Resume",
					"target": "Resume"
				},
				{
					"pattern": "calendar/appointment:?query:",
					"name": "NewAppointment",
					"target": "Calendar"
				},
				{
					"pattern": "calendar/appointment/{appointment}",
					"name": "Appointment",
					"target": "Calendar"
				},
				{
					"pattern": "calendar/:view:",
					"name": "Calendar",
					"target": "Calendar"
				},
				{
					"pattern": "games/hanoitower/:discs:",
					"name": "HanoiTower",
					"target": "HanoiTower"
				},
				{
					"pattern": "games/tictactoe/:level:",
					"name": "TicTacToe",
					"target": "TicTacToe"
				},
				{
					"pattern": "games/minesweeper/:level:",
					"name": "Minesweeper",
					"target": "Minesweeper"
				},
				{
					"pattern": "algorithms/:article:",
					"name": "Algorithms",
					"target": "Algorithms"
				},
				{
					"pattern": "sandbox",
					"name": "Sandbox",
					"target": "Sandbox"
				},
				{
					"pattern": "games",
					"name": "Games",
					"target": "Games"
				}
			],
			"targets": {
				"Games": {
					"name": "Games",
					"title": "{i18n>ttlGames}"
				},
				"Home": {
					"name": "Home",
					"title": "harelyshau.dev"
				},
				"Resume": {
					"name": "Resume",
					"title": "{/FirstName} {/LastName}"
				},
				"Calendar": {
					"name": "Calendar",
					"title": "{i18n>ttlPavelsCalendar}"
				},
				"HanoiTower": {
					"parent": "Games",
					"name": "HanoiTower",
					"title": "{i18n>ttlHanoiTower}",
					"controlId": "game",
					"controlAggregation": "pages"
				},
				"Minesweeper": {
					"parent": "Games",
					"name": "Minesweeper",
					"title": "{i18n>ttlMinesweeper}",
					"controlId": "game",
					"controlAggregation": "pages"
				},
				"TicTacToe": {
					"parent": "Games",
					"name": "TicTacToe",
					"title": "{i18n>ttlTicTacToe}",
					"controlId": "game",
					"controlAggregation": "pages"
				},
				"Algorithms": {
					"name": "Algorithms",
					"title": "{= ${/Article/Title} || ${i18n>ttlAlgorithms} }"
				},
				"Sandbox": {
					"name": "Sandbox",
					"title": "{i18n>ttlSandbox}"
				},
				"NotFound": {
					"name": "NotFound",
					"title": "{i18n>ttlPageNotFound}"
				}
			}
		},
		"resources": {
			"css": [
				{ "uri": "css/style.css" },
				{ "uri": "css/home.css" },
				{ "uri": "css/resume.css" },
				{ "uri": "css/calendar.css" },
				{ "uri": "css/algorithms.css" },
				{ "uri": "css/hanoitower.css" },
				{ "uri": "css/minesweeper.css" },
				{ "uri": "css/tictactoe.css" }
			]
		},
		"handleValidation": true
	}
}
