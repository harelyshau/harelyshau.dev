{
	"_version": "1.60.0",
	"start_url": "index.html",
	"sap.app": {
		"id": "pharelyshau",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"i18n": {
			"bundleUrl": "i18n/i18n.properties",
			"supportedLocales": ["en", "de", "ru"],
			"fallbackLocale": "en"
		},
		"dataSources": {},
		"offline": true
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"fullWidth": true
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.120.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.f": {},
				"sap.ui.layout": {},
				"sap.ui.unified": {},
				"sap.tnt": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "pharelyshau.i18n.i18n",
					"supportedLocales": ["en", "de", "ru"],
					"fallbackLocale": "en"
				}
			}
		},
		"rootView": {
			"viewName": "pharelyshau.view.App",
			"type": "XML",
			"id": "app",
			"async": true
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"type": "View",
				"viewType": "XML",
				"path": "pharelyshau.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"bypassed": {
					"target": ["notFound"]
				},
				"async": true
			},
			"routes": [
				{
					"pattern": "",
					"name": "Home",
					"target": "home"
				},
				{
					"pattern": "resume",
					"name": "Resume",
					"target": "resume"
				},
				{
					"pattern": "calendar/:view:",
					"name": "Calendar",
					"target": "calendar"
				},
				{
					"pattern": "hanoitower",
					"name": "HanoiTower",
					"target": "hanoitower"
				},
				{
					"pattern": "minesweeper",
					"name": "Minesweeper",
					"target": "minesweeper"
				},
				{
					"pattern": "algorithms/:articleId:",
					"name": "Algorithms",
					"target": "algorithms"
				},
				{
					"pattern": "sandbox",
					"name": "Sandbox",
					"target": "sandbox"
				}
			],
			"targets": {
				"home": {
					"id": "home",
					"name": "Home",
					"title": "harelyshau.dev"
				},
				"resume": {
					"id": "resume",
					"name": "Resume",
					"title": "{/FirstName} {/LastName}"
				},
				"calendar": {
					"id": "calendar",
					"name": "Calendar",
					"title": "{i18n>ttlPavelsCalendar}"
				},
				"hanoitower": {
					"id": "hanoitower",
					"name": "HanoiTower",
					"title": "{i18n>ttlHanoiTower}"
				},
				"minesweeper": {
					"id": "minesweeper",
					"name": "Minesweeper",
					"title": "{i18n>ttlMinesweeper}"
				},
				"algorithms": {
					"id": "algorithms",
					"name": "Algorithms",
					"title": "{= ${/Article/Title} || ${i18n>ttlAlgorithms} }"
				},
				"sandbox": {
					"id": "sandbox",
					"name": "Sandbox",
					"title": "{i18n>ttlSandbox}"
				},
				"notFound": {
					"id": "notFound",
					"name": "NotFound",
					"title": "{i18n>ttlPageNotFound}"
				}
			}
		},
		"resources": {
			"css": [
				{ "uri": "css/style.css" },
				{ "uri": "css/home.css" },
				{ "uri": "css/resume.css" },
				{ "uri": "css/calendar.css" },
				{ "uri": "css/algorithms.css" },
				{ "uri": "css/hanoitower.css" },
				{ "uri": "css/minesweeper.css" }
			]
		},
		"handleValidation": true
	}
}
